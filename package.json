{
  "name": "mern-hsms-monorepo",
  "private": true,
  "version": "1.0.0",
  "workspaces": [],
  "scripts": {
    "start": "npm --prefix backend start",
    "server": "npm --prefix backend run server",
    "setup": "npm --prefix backend run setup",
    "frontend": "npm --prefix frontend start",
    "install-frontend": "npm --prefix frontend install",
    "install-backend": "npm --prefix backend install",
    "install": "npm run install-backend && npm run install-frontend",
    "dev": "concurrently \"npm:server\" \"npm:frontend\""
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}


